{
  "name": "mantine-vite-template",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
    "electron:build": "npm build && tsc -p electron && electron-builder"
  },
  "description": "Official [Mantine](https://mantine.dev/) + [Vite](https://vitejs.dev/) template.",
  "main": "electron.js",
  "author": "R19",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RegnaR19/vite-mantine/issues"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/carousel": "^5.10.2",
    "@mantine/core": "^5.10.1",
    "@mantine/dates": "^5.10.2",
    "@mantine/dropzone": "^5.10.2",
    "@mantine/form": "^5.10.2",
    "@mantine/hooks": "^5.10.1",
    "@mantine/modals": "^5.10.2",
    "@mantine/notifications": "^5.10.2",
    "@mantine/nprogress": "^5.10.2",
    "@mantine/prism": "^5.10.2",
    "@mantine/spotlight": "^5.10.2",
    "@mantine/tiptap": "^5.10.2",
    "@tabler/icons": "^1.119.0",
    "@tiptap/extension-link": "^2.0.0-beta.209",
    "@tiptap/react": "^2.0.0-beta.209",
    "@tiptap/starter-kit": "^2.0.0-beta.209",
    "@types/electron-devtools-installer": "^2.2.2",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.0",
    "embla-carousel-react": "^7.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.0",
    "redux": "^4.2.1",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.0.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.1.1",
    "electron-builder": "^22.10.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "wait-on": "^7.0.1"
  }
}