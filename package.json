{
  "name": "mantine-vite-template",
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "url": "http://localhost:5173/home",
      "webRoot": "${workspaceRoot}",
      "runtimeArgs": [
        "--disable-web-security"
      ]
    }
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/carousel": "^5.10.2",
    "@mantine/core": "^5.10.1",
    "@mantine/dates": "^5.10.2",
    "@mantine/dropzone": "^5.10.2",
    "@mantine/form": "^5.10.2",
    "@mantine/hooks": "^5.10.1",
    "@mantine/modals": "^5.10.2",
    "@mantine/notifications": "^5.10.2",
    "@mantine/nprogress": "^5.10.2",
    "@mantine/prism": "^5.10.2",
    "@mantine/spotlight": "^5.10.2",
    "@mantine/tiptap": "^5.10.2",
    "@tabler/icons": "^1.119.0",
    "@tiptap/extension-link": "^2.0.0-beta.209",
    "@tiptap/react": "^2.0.0-beta.209",
    "@tiptap/starter-kit": "^2.0.0-beta.209",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "electron-squirrel-startup": "^1.0.0",
    "embla-carousel-react": "^7.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.0",
    "redux": "^4.2.1",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "electron": "^23.1.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "description": "Official [Mantine](https://mantine.dev/) + [Vite](https://vitejs.dev/) template.",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RegnaR19/vite-mantine.git"
  },
  "author": "R19",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RegnaR19/vite-mantine/issues"
  },
  "homepage": "https://github.com/RegnaR19/vite-mantine#readme"
}